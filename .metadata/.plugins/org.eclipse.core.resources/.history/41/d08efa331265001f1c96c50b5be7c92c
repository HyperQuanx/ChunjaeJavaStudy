package chap4;

public class gugudanss {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int cnt = 0;
		for(int j = 0; j <= 9; j++) {
			if (cnt == 6) {
				System.out.println();
			}
			for(int i = 2; i <= 9; i++) {
				if (j == 0) {
					System.out.print(String.format("%9s",i + "단"));
					if (i == 9) {
						System.out.println();
						cnt++;
					}
				} else {
					System.out.print(String.format("%10s", i + "X" + j + "= " + i * j));
					if (i == 9) {
						System.out.println();
					}
				}
			}
		}

	}
}
// 9일때 내려쓰기