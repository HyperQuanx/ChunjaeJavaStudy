package chap4;

public class gugudan {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for (int num4 = 2; num4 <=5; num4++) {
			System.out.print(num4 + "단" + "       ");
		}
		
		System.out.println("");
		for (int num = 1; num <= 9; num++) {
			for(int num2 = 2; num2 <=5; num2++) {
				System.out.print(num2 + "X" + num + "= " + num * num2 + "    ");
				System.out.print("");
				if (num2 == 5) {
					System.out.println("");
				}
			}
			
		}
		for (int num4 = 6; num4 <=9; num4++) {
			System.out.print(num4 + "단" + "       ");
		}
		System.out.println("");
		for (int num = 1; num <= 9; num++) {
			for(int num2 = 6; num2 <=9; num2++) {
				System.out.print(num2 + "X" + num + "= " + num * num2 + "    ");
				System.out.print("");
				if (num2 == 9) {
					System.out.println("");
				}
			}
		}
	}

}
