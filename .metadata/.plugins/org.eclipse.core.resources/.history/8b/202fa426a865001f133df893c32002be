package chap4;

public class asdasdf {

	public static void main(String[] args) {
		/*
		 * char [] a = new char[3]; a[0] = '1'; a[1] = '2'; a[2] = '3';
		 * 
		 * 
		 * char [] b = {'1',2,'3'}; System.out.println(a); System.out.println(b);
		 */
//		
//		int a = 3;
//		int b = a;
//		int c = b;
//		
//		System.out.println(a + ":" + b + ":"+ c);
//		
//		a = 4;
//		
//		System.out.println(a + ":" + b + ":"+ c);
//		
//		int [] x = {1, 2, 3, 4};
//		int [] z = x;
//		
//		System.out.println(x[0]);
//		System.out.println(z[0]);
//		
//		x[0] = 10;
//		
//		System.out.println(x[0]);
//		System.out.println(z[0]);
//		
//		float num1 = 11;
//		double num2 = 11;
//		int num3 = 10;
//		int num4 = -6;
//		int num5 = 69;
//		int num6 = 6113;
		int num7 = -11;
		
//		System.out.println(num1/3);
//		System.out.println(num2/3);
//		System.out.println(num3 >>> 2);
//		System.out.println(num4 << 2);
//		System.out.println(num5 << 2);
//		System.out.println(num4 >> 2);
//		System.out.println(num4 >>> 2);
//		System.out.println(num6 >> 2);
//		System.out.println(num6 >>> 2);
		System.out.println(-1 >> 2);
		System.out.println(-1 >>> 2);
		System.out.println(-2 >> 2);
		System.out.println(-2 >>> 2);
		System.out.println(-3 >> 2);
		System.out.println(-3 >>> 2);
		System.out.println(-4 >> 2);
		System.out.println(-4 >>> 2);
		System.out.println(-5 >> 2);
		System.out.println(-5 >>> 2);
		System.out.println(-6 >> 2);
		System.out.println(-6 >>> 2);
		System.out.println(-7 >> 2);
		System.out.println(-7 >>> 2);
		System.out.println(-8 >> 2);
		System.out.println(-8 >>> 2);
		System.out.println(-9 >> 2);
		System.out.println(-9 >>> 2);
		System.out.println(-10 >> 2);
		System.out.println(-10>>> 2);
		System.out.println(-1457 >> 2);
		System.out.println(-1457 >>> 2);
	}

}



// 선언만 하면 메모리 사용 X   ex) int a = 0;  <-- 메모리 사용하지 않음