package chap4;

public class gugudanss {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int j = 2; j <= 9; j++) {
			for(int i = 0; i <= 9; i++) {
				if (i == 0) {
					System.out.print(String.format("%10s", i + "ë‹¨"));
					continue;
				}
				System.out.println(String.format("%10s", j + "X" + i + "= " + i * j));
			}
		}

	}
}
